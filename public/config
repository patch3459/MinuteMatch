
// public/config

module.exports = {
  adjustNum: (num1, num2, num3) => {return num1 * (num2 / num3);},
  add: (num1, num2) => {return num1 + num2;},
  subtract: (num1, num2) => {return num1 - num2;},
  percentage: (num1, num2) => {return Math.floor(100 * num1 / num2);},
  onePercentage: (num) => {return Math.round( 100 * num);}
  
}

